<!DOCTYPE feature SYSTEM "gdb-target.dtd">
<feature name="org.gnu.gdb.intelgt.arf9">

  <reg name="a0"     bitsize="256" type="uint256"/>
  <reg name="acc0"   bitsize="256" type="uint256"/>
  <reg name="acc1"   bitsize="256" type="uint256"/>
  <reg name="acc2"   bitsize="256" type="uint256"/>
  <reg name="acc3"   bitsize="256" type="uint256"/>
  <reg name="acc4"   bitsize="256" type="uint256"/>
  <reg name="acc5"   bitsize="256" type="uint256"/>
  <reg name="acc6"   bitsize="256" type="uint256"/>
  <reg name="acc7"   bitsize="256" type="uint256"/>
  <reg name="acc8"   bitsize="256" type="uint256"/>
  <reg name="acc9"   bitsize="256" type="uint256"/>
  <reg name="f0"     bitsize="32"  type="uint32"/>
  <reg name="f1"     bitsize="32"  type="uint32"/>
  <!-- FIXME: BSpec says this is RO, should we still enable write
       to this reg? -->
  <reg name="ce"     bitsize="32"  type="uint32"/>
  <reg name="sp"     bitsize="128" type="uint128"/>
  <reg name="sr0"    bitsize="128" type="uint128"/>
  <!-- FIXME: only cr0.2 is available for edit,  do we really want
       to expose cr0? -->
  <reg name="cr0"    bitsize="128" type="uint128"/>
  <reg name="ip"     bitsize="32"  type="uint32"/>

  <reg name="tdr"    bitsize="128" type="uint128"/>

  <reg name="tm0"    bitsize="128" type="uint128"/>
  <reg name="emask"  bitsize="32"  type="uint32"/>
  <reg name="iemask" bitsize="32"  type="uint32"/>

  <reg name="mme0"   bitsize="256" type="uint256"/>
  <reg name="mme1"   bitsize="256" type="uint256"/>
  <reg name="mme2"   bitsize="256" type="uint256"/>
  <reg name="mme3"   bitsize="256" type="uint256"/>
  <reg name="mme4"   bitsize="256" type="uint256"/>
  <reg name="mme5"   bitsize="256" type="uint256"/>
  <reg name="mme6"   bitsize="256" type="uint256"/>
  <reg name="mme7"   bitsize="256" type="uint256"/>
</feature>
